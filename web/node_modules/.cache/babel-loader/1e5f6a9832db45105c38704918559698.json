ent":["<template>\r\n  <div id=\"togetherContent\">\r\n    <div class=\"content\">\r\n      <div class=\"list\" v-for=\"(item, index) in listData\" :key=\"index\">\r\n        <el-avatar\r\n          :size=\"55\"\r\n          :src=\"item.avatar\"\r\n          style=\"display: inline-block; position: absolute; left: 0; top: 20px\"\r\n        ></el-avatar>\r\n        <p class=\"tit\">{{ item.articleTitle }}</p>\r\n        <p class=\"name\">{{ item.nickname }}</p>\r\n        <p class=\"time\">发布时间：{{ item.createdTime }}</p>\r\n        <p class=\"huifu\">回复 {{ item.articleCommentCount }}</p>\r\n        <p class=\"zan\">赞 {{ item.articleLikeCount }}</p>\r\n        <p class=\"liulan\">浏览量 {{ item.articleViewCount }}</p>\r\n      </div>\r\n      <!-- <div class=\"list\"></div>\r\n      <div class=\"list\"></div> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"indexContent\",\r\n  data() {\r\n    return {\r\n      circleUrl: require(\"@/assets/img1.webp\"),\r\n      listData: [],\r\n      id: \"5\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n  },\r\n  methods: {\r\n    getData() {\r\n      var _self = this;\r\n      var url = \"http://localhost:8080/category/\";\r\n      var categoryId = this.id;\r\n      _self.$axios\r\n        .get(url + categoryId, { params: { categoryId: categoryId } })\r\n        .then((res) => {\r\n          console.log(res.data.data);\r\n          if (res.status == 200) {\r\n            if (res.data.code == 0) {\r\n              // console.log(res.data.data.articleTitle);\r\n              let da = res.data.data;\r\n              _self.listData = da;\r\n            }\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n#togetherContent {\r\n  width: 100%;\r\n  .content {\r\n    width: 96%;\r\n    height: 480px;\r\n    margin: 20px auto 0;\r\n    .list {\r\n      width: 100%;\r\n      height: 90px;\r\n      margin-bottom: 5px;\r\n      border-bottom: 2px solid #eee;\r\n      //   background: #eee;\r\n      position: relative;\r\n      .tit {\r\n        position: absolute;\r\n        left: 65px;\r\n        top: 25px;\r\n        font-size: 15px;\r\n        font-weight: 500;\r\n        color: rgb(83, 82, 82);\r\n      }\r\n      .name {\r\n        position: absolute;\r\n        left: 65px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(241, 45, 45);\r\n      }\r\n      .time {\r\n        position: absolute;\r\n        left: 200px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(82, 80, 80);\r\n      }\r\n      .huifu {\r\n        width: 80px;\r\n        position: absolute;\r\n        right: 180px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        border-right: 2px solid rgb(126, 125, 125);\r\n        color: rgb(82, 80, 80);\r\n      }\r\n      .zan {\r\n        width: 80px;\r\n        border-right: 2px solid rgb(126, 125, 125);\r\n        position: absolute;\r\n        right: 100px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(82, 80, 80);\r\n      }\r\n      .liulan {\r\n        position: absolute;\r\n        right: 0px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(82, 80, 80);\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components/together"}]}