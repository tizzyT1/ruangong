A;AAMA,KAjBA;;AAkBA,IAAA,eAAA,EAAA,YAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AACA,KApBA;AAqBA,IAAA,gBAAA,EAAA,UAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,CAAA;AACA,KAvBA;AAwBA,IAAA,kBAAA,EAAA,UAAA,CAAA,EAAA;AACA,UAAA,EAAA,GAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,IAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,KA5BA;AA6BA,IAAA,cAAA,EAAA,UAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,wBAAA,CAAA,CAAA,IAAA,GAAA,GAAA;AACA,KA/BA;;AAgCA,IAAA,KAAA,GAAA;AACA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,GAAA,GAAA,qCAAA;;AACA,MAAA,KAAA,CAAA,MAAA,CACA,GADA,CACA,GADA,EAEA,IAFA,CAEA,GAAA,IAAA;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,SAAA,CAAA;AAAA,YAAA,IAAA,EAAA,OAAA;AAAA,YAAA,GAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAAA,WAAA;AACA;AACA,OAPA,EAQA,KARA,CAQA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA;AAWA,KA/CA;;AAgDA,IAAA,WAAA,GAAA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,GAAA,GAAA,mCAAA,CAFA,CAGA;;AAEA,MAAA,KAAA,CAAA,MAAA,CACA,GADA,CACA,GADA,EAEA,IAFA,CAEA,GAAA,IAAA;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,GAAA,CAAA,IAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,cAAA,IAAA,GAAA,EAAA;AACA,UAAA,IAAA,CAAA,OAAA,CAAA,IAAA,IAAA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA;AACA,WAFA;AAGA,UAAA,KAAA,CAAA,UAAA,GAAA,IAAA;AACA;AACA,OAZA,EAaA,KAbA,CAaA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAfA;AAgBA,KArEA;;AAsEA,IAAA,OAAA,CAAA,MAAA,EAAA;AACA,WAAA,OAAA,GAAA,CAAA,KAAA,OAAA,CADA,CAEA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,GAAA,GAAA,kCAAA,CAJA,CAKA;;AAEA,MAAA,KAAA,CAAA,MAAA,CACA,GADA,CACA,GADA,EACA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,QAAA,EAAA;AAAA;AAAA,OADA,EAEA,IAFA,CAEA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA;AACA,YAAA,KAAA,CAAA,WAAA,GAFA,CAGA;AACA;;AACA;AACA;AACA,OAZA,EAaA,KAbA,CAaA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAfA;AAgBA,KA7FA;;AA8FA,IAAA,SAAA,CAAA,MAAA,EAAA;AACA,WAAA,OAAA,GAAA,CAAA,KAAA,OAAA,CADA,CAEA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,GAAA,GAAA,qCAAA,CAJA,CAKA;;AAEA,MAAA,KAAA,CAAA,MAAA,CACA,GADA,CACA,GADA,EACA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,QAAA,EAAA;AAAA;AAAA,OADA,EAEA,IAFA,CAEA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,WAAA,GADA,CAEA;AACA;AACA;;AACA;AACA;AACA,OAZA,EAaA,KAbA,CAaA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAfA;AAgBA,KArHA;;AAsHA,IAAA,OAAA,GAAA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,GAAA,GAAA,iCAAA;AACA,UAAA,UAAA,GAAA,KAAA,EAAA,CAHA,CAIA;;AACA,MAAA,KAAA,CAAA,MAAA,CACA,GADA,CACA,GAAA,GAAA,UADA,EACA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,UAAA,EAAA;AAAA;AAAA,OADA,EAEA,IAFA,CAEA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA;AACA,gBAAA,EAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,EAAA;AACA;AACA;AACA,OAXA,EAYA,KAZA,CAYA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAdA;AAeA,KA1IA;;AA2IA,IAAA,QAAA,CAAA,GAAA,EAAA;AACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,QAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,GAAA,EAAA,GAAA,CAAA;AAAA;AAAA,OAAA;AACA;;AA9IA;AAlCA,CAAA","sourcesContent":["<template>\r\n  <div id=\"indexContent\">\r\n    <div class=\"content\">\r\n      <div class=\"list\" v-for=\"(item, index) in listData\" :key=\"index\">\r\n        <el-avatar\r\n          :size=\"55\"\r\n          :src=\"item.avatar\"\r\n          style=\"\r\n            display: inline-block;\r\n            position: absolute;\r\n            left: 0;\r\n            top: 20px;\r\n            cursor: pointer;\r\n          \"\r\n        ></el-avatar>\r\n        <p class=\"tit\" @click=\"goDetail(item)\">{{ item.articleTitle }}</p>\r\n        <p class=\"name\" @click=\"goDetail(item)\">{{ item.nickname }}</p>\r\n\r\n        <p class=\"time\">发布时间：{{ item.createdTime }}</p>\r\n        <p class=\"huifu\">回复 {{ item.articleCommentCount }}</p>\r\n        <p style=\"position: absolute; right: 90px; top: 60px\">|</p>\r\n        <p style=\"position: absolute; right: 160px; top: 60px\">|</p>\r\n        <p class=\"zan\">赞 {{ item.articleLikeCount }}</p>\r\n        <p class=\"liulan\">浏览量 {{ item.articleViewCount }}</p>\r\n        <el-button\r\n          style=\"position: relative; left: -235px; top: 35px\"\r\n          type=\"info\"\r\n          display=\"inline-block\"\r\n          @click=\"noguanzhu(item.articleUserId)\"\r\n          v-if=\"followList.indexOf(String(item.articleUserId)) > -1\"\r\n          >已关注\r\n        </el-button>\r\n        <el-button\r\n          style=\"position: relative; left: -235px; top: 35px\"\r\n          display=\"inline-block\"\r\n          type=\"primary\"\r\n          @click=\"guanzhu(item.articleUserId)\"\r\n          v-else\r\n          >关注 <i class=\"el-icon-plus\"></i>\r\n        </el-button>\r\n        <el-button\r\n          style=\"position: relative; left: -230px; top: 35px\"\r\n          display=\"inline-block\"\r\n          type=\"primary\"\r\n          @click=\"message(item.articleUserId)\"\r\n          >私信\r\n        </el-button>\r\n      </div>\r\n      <!-- <div class=\"list\"></div>\r\n      <div class=\"list\"></div> -->\r\n      <el-dialog title=\"聊天框\" :visible.sync=\"dialogVisible\" width=\"80%\">\r\n        <div style=\"position: absolute; left: 0px; top: 0px\">\r\n          与{{ chatId }}聊天\r\n        </div>\r\n\r\n        <div class=\"left\">\r\n          <div class=\"content\"></div>\r\n          <textarea name=\"\" id=\"\" cols=\"30\" rows=\"10\"></textarea>\r\n        </div>\r\n        <!-- <div class=\"right\">\r\n          <h3>在线好友</h3>\r\n          <li>\r\n\r\n          </li>\r\n        </div> -->\r\n\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"dialogVisible = false\"\r\n            >确 定</el-button\r\n          >\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"indexContent\",\r\n  data() {\r\n    return {\r\n      circleUrl: require(\"@/assets/img1.webp\"),\r\n      id: \"1\",\r\n      display: true,\r\n      chatId: 0,\r\n      listData: [\r\n        // {\r\n        //   liIndex: \"\",\r\n        //   imgSrc: \"\",\r\n        //   tit: \"\",\r\n        //   name: \"\",\r\n        //   time: \"\",\r\n        //   huifu: \"\",\r\n        //   zan: \"\",\r\n        //   liulan: \"\",\r\n        // },\r\n      ],\r\n      followList: [],\r\n      hasRed: false,\r\n      dialogVisible: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n    this.guanzhuList();\r\n\r\n    setInterval(() => {\r\n      this.isRed();\r\n    }, 3000);\r\n    //console.log('全局红点',this.$red)\r\n  },\r\n  methods: {\r\n    message(id) {\r\n      this.dialogVisible = true;\r\n      this.chatId = id;\r\n\r\n      // this.websocket = new WebSocket(`ws://localhost:8081/${id}`);\r\n      // this.websock.onopen = this.websocketonopen;\r\n      // this.websock.onerror = this.websocketonerror;\r\n      // this.websock.onmessage = this.websocketonmessage;\r\n      // this.websock.onclose = this.websocketclose;\r\n      let _self = this;\r\n      this.$router.push({\r\n        name: \"chat\",\r\n        params: {\r\n          userId: _self.chatId,\r\n        },\r\n      });\r\n    },\r\n    websocketonopen: function () {\r\n      console.log(\"WebSocket连接成功\");\r\n    },\r\n    websocketonerror: function (e) {\r\n      console.log(\"WebSocket连接发生错误\", e);\r\n    },\r\n    websocketonmessage: function (e) {\r\n      var da = JSON.parse(e.data);\r\n      console.log(da);\r\n      this.message = da;\r\n    },\r\n    websocketclose: function (e) {\r\n      console.log(\"connection closed (\" + e.code + \")\");\r\n    },\r\n    isRed() {\r\n      //console.log(777)\r\n      var _self = this;\r\n      var url = \"http://localhost:8080/hasReadNotice\";\r\n      _self.$axios\r\n        .get(url)\r\n        .then((res) => {\r\n          if (res.status == 200) {\r\n            //console.log(\"红点问题\", res.data);\r\n            _self.$store.amendType({ type: \"isRed\", blo: res.data.data });\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    guanzhuList() {\r\n      var _self = this;\r\n      var url = \"http://localhost:8080/follow/list\";\r\n      //var categoryId = this.id;\r\n\r\n      _self.$axios\r\n        .get(url)\r\n        .then((res) => {\r\n          if (res.status == 200) {\r\n            console.log(\"关注列表\", res.data);\r\n            let args = res.data.data;\r\n            let list = [];\r\n            args.forEach((item) => {\r\n              list.push(item.userId);\r\n            });\r\n            _self.followList = list;\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    guanzhu(userId) {\r\n      this.display = !this.display;\r\n      //console.log(userId);\r\n      var _self = this;\r\n      var url = \"http://localhost:8080/add/follow\";\r\n      //var categoryId = this.id;\r\n\r\n      _self.$axios\r\n        .get(url, { params: { followId: userId } })\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          if (res.status == 200) {\r\n            if (res.data.code == 0) {\r\n              //console.log(res.data.data.articleTitle);\r\n              _self.guanzhuList();\r\n              // let da = res.data.data;\r\n              // _self.listData = da;\r\n            }\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    noguanzhu(userId) {\r\n      this.display = !this.display;\r\n      //console.log(userId);\r\n      var _self = this;\r\n      var url = \"http://localhost:8080/cancel/follow\";\r\n      //var categoryId = this.id;\r\n\r\n      _self.$axios\r\n        .get(url, { params: { followId: userId } })\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          if (res.status == 200) {\r\n            if (res.data.code == 0) {\r\n              _self.guanzhuList();\r\n              //console.log(res.data.data.articleTitle);\r\n              // let da = res.data.data;\r\n              // _self.listData = da;\r\n            }\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    getData() {\r\n      var _self = this;\r\n      var url = \"http://localhost:8080/category/\";\r\n      var categoryId = this.id;\r\n      // console.log(categoryId, \"ipppppppppppp\");\r\n      _self.$axios\r\n        .get(url + categoryId, { params: { categoryId: categoryId } })\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          if (res.status == 200) {\r\n            if (res.data.code == 0) {\r\n              //console.log(res.data.data.articleTitle);\r\n              let da = res.data.data;\r\n              _self.listData = da;\r\n            }\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    goDetail(num) {\r\n      // localStorage.setItem(\"articleId\", num.articleId);\r\n      this.$router.push({ name: \"Detail\", query: { url: num.articleId } });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n#indexContent {\r\n  width: 100%;\r\n  .el-dialog {\r\n    height: 600px;\r\n    .left {\r\n      width: 60%;\r\n      border: 1px solid black;\r\n      display: inline-block;\r\n    }\r\n    .right {\r\n      width: 20%;\r\n      //border: 1px solid black;\r\n      display: inline-block;\r\n    }\r\n  }\r\n  .content {\r\n    width: 96%;\r\n    height: 480px;\r\n    margin: 20px auto 0;\r\n    .list {\r\n      width: 100%;\r\n      height: 90px;\r\n      margin-bottom: 5px;\r\n      border-bottom: 2px solid #eee;\r\n\r\n      position: relative;\r\n      .tit {\r\n        position: absolute;\r\n        left: 65px;\r\n        top: 25px;\r\n        font-size: 15px;\r\n        font-weight: 500;\r\n        color: rgb(83, 82, 82);\r\n        cursor: pointer;\r\n      }\r\n      .name {\r\n        position: absolute;\r\n        left: 65px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(51, 49, 49);\r\n        cursor: pointer;\r\n      }\r\n      .time {\r\n        position: relative;\r\n        left: 10px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(82, 80, 80);\r\n      }\r\n      .huifu {\r\n        width: 80px;\r\n        position: absolute;\r\n        right: 170px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        // border-right: 2px solid rgb(126, 125, 125);\r\n        color: rgb(82, 80, 80);\r\n      }\r\n      .zan {\r\n        width: 80px;\r\n        // border-right: 2px solid rgb(126, 125, 125);\r\n        position: absolute;\r\n        right: 90px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(82, 80, 80);\r\n      }\r\n      .liulan {\r\n        position: absolute;\r\n        right: 25px;\r\n        top: 60px;\r\n        font-size: 14px;\r\n        color: rgb(82, 80, 80);\r\n      }\r\n    }\r\n  }\r\n}\r\n/deep/.el-button {\r\n  display: inline-block;\r\n  position: absolute;\r\n  left: 20px;\r\n  line-height: 1;\r\n  white-space: nowrap;\r\n  cursor: pointer;\r\n  background: #9a9898;\r\n  border: 1px solid #dcdfe6;\r\n  color: #606266;\r\n  -webkit-appearance: none;\r\n  text-align: center;\r\n  box-sizing: border-box;\r\n  outline: 0;\r\n  margin: 0;\r\n  transition: 0.1s;\r\n  font-weight: 500;\r\n  padding: 5px 8px;\r\n  font-size: 14px;\r\n  border-radius: 4px;\r\n  color: #87baf1;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/index"}]}